%p Create a New End Entity Certificate

= form_for @certificate, url: admin_end_entity_certificates_path(@end_entity) do |f|
	
	= render partial: 'form', object: f
	
	.row
		.large-4.columns
			&nbsp;
		.large-4.columns
			.right
				= f.submit 'Create', {class: 'button small'}
		.large-4.columns
			&nbsp;

= link_to 'Cancel', admin_end_entities_path, {class: 'button small'}